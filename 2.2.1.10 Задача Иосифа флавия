n = int(input())
k = int(input())
solders = [i for i in range(1, n + 1)]
cursor = 0
while len(solders) > 1:
    cursor = (cursor + k - 1) % len(solders)
    solders.pop(cursor)
    cursor = cursor % len(solders)
print(*solders)