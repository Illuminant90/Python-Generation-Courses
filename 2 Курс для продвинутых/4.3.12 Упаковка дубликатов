count = 1
for i in range(1, len(line)):
    if line[i] != line[i - 1]:
        count += 1
output = [[] for _ in range(count)]
count = 0
output[count].append(line[0])
for i in range(1, len(line)):
    if line[i] != line[i - 1]:
        count += 1
        output[count].append(line[i])
    else:
        output[count].append(line[i])
print(output)